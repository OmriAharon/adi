{
  "name": "adi",
  "version": "0.0.15",
  "homepage": "www.adigoldshtein.com",
  "description": "The web component for BIM360 Quality",
  "scripts": {
    "lint": "node_modules/.bin/npm-run-all --parallel lint:*",
    "lint:js": "node_modules/.bin/eslint \"**/*.js\" -c .eslintrc",
    "fix:lint:js": "npm run lint:js -- --fix",
    "lint:sass": "node_modules/.bin/sass-lint -c .sass-lint.yml -v -q",
    "start": "node_modules/.bin/cross-env BABEL_ENV=local HOST_PROTOCOL=http NODE_ENV=local NODE_PORT=3001 HOST_ADDRESS=local-adigoldshtein.com node --harmony ./scripts/start.js",
    "start:refresh": "npm run setup:refresh && npm start",
    "start:environments": "node_modules/.bin/cross-env BABEL_ENV=local HOST_PROTOCOL=http NODE_PORT=3001 HOST_ADDRESS=local-adigoldshtein.com node --harmony ./scripts/start.js",
    "start:dev": "node_modules/.bin/cross-env NODE_ENV=dev npm run start:environments",
    "build": "node_modules/.bin/cross-env BABEL_ENV=es NODE_ENV=local HOST_PROTOCOL=http NODE_PORT=3001 HOST_ADDRESS=local-adigoldshtein.com node --harmony ./node_modules/.bin/webpack --config ./webpack-config/webpack.config.local.js --progress",
    "build:stats": "node_modules/.bin/cross-env STATS=true npm run build",
    "build:environments": "node_modules/.bin/cross-env BABEL_ENV=es HOST_PROTOCOL=https NODE_PORT=443 node --harmony ./node_modules/.bin/webpack --config ./webpack-config/webpack.config.environments.js --progress",
    "build:dev": "node_modules/.bin/cross-env NODE_ENV=dev npm run build:environments",
    "build:dev:stats": "node_modules/.bin/cross-env STATS=true npm run build:dev",
    "build:production": "node_modules/.bin/cross-env NODE_ENV=production HOST_ADDRESS=adigoldshtein.com npm run build:environments",
    "build:production:stats": "node_modules/.bin/cross-env STATS=true npm run build:production",
    "setup:refresh": "npm prune && npm install",
    "prepush": "npm run lint",
    "licenses": "node_modules/.bin/license-checker --json --production > licenses.json"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.20.1",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.2.3",
    "express": "^4.18.2",
    "favicons-webpack-plugin": "0.0.7",
    "googleapis": "^57.0.0",
    "history": "3.3.0",
    "i18n-react": "0.3.0-ts1",
    "nodemailer": "^4.4.1",
    "prop-types": "15.5.10",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-flags-select": "1.1.0",
    "react-ga": "^2.3.5",
    "react-google-maps": "^9.4.2",
    "react-redux": "5.0.4",
    "react-router": "^3.2.0",
    "redux": "3.6.0",
    "scrollreveal": "^3.3.6",
    "seamless-immutable": "7.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.5.0",
    "chromedriver": "2.33.2",
    "clean-webpack-plugin": "0.1.16",
    "cross-env": "3.2.4",
    "css-loader": "0.28.4",
    "eslint": "4.2.0",
    "eslint-plugin-no-unsafe-innerhtml": "1.0.16",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-inline-chunk-plugin": "1.1.1",
    "html-webpack-plugin": "2.29.0",
    "http-server": "14.1.1",
    "interpolate-loader-options-webpack-plugin": "0.0.1",
    "jquery": "^1.11.0",
    "json-loader": "0.5.4",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "postcss-loader": "2.0.6",
    "preload-webpack-plugin": "^2.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.6",
    "sass-variables-loader": "^0.1.3",
    "script-ext-html-webpack-plugin": "1.8.8",
    "source-map-url": "0.4.0",
    "style-loader": "0.18.2",
    "url-loader": "^0.5.9",
    "watch-run": "1.2.5",
    "webpack": "3.3.0",
    "webpack-bundle-analyzer": "2.8.3",
    "webpack-dashboard": "1.0.0-5",
    "webpack-dev-server": "2.5.1",
    "yaml-loader": "0.5.0"
  }
}
